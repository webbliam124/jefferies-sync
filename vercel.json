{
  "functions": {
    "api/vapi_handler.py": {
      "runtime": "python3.11"
    },
    "api/rex_sync.py": {
      "runtime": "python3.11",
      "maxDuration": 60
    }
  },
  "routes": [
    {
      "src": "/api/vapi_handler",
      "dest": "api/vapi_handler.py"
    },
    {
      "src": "/api/rex_sync",
      "dest": "api/rex_sync.py"
    }
  ],
  "crons": [
    {
      "path": "/api/rex_sync",
      "schedule": "0 9 * * *"
    }
  ]
}